local ScreenGui = Instance.new("ScreenGui")
local Frame = Instance.new("Frame")
local TextLabel = Instance.new("TextLabel")
local UICorner = Instance.new("UICorner")
local UICorner_2 = Instance.new("UICorner")
local eclipse = Instance.new("TextButton")
local UICorner_3 = Instance.new("UICorner")
local Vynixu = Instance.new("TextButton")
local UICorner_4 = Instance.new("UICorner")

--Properties:

ScreenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

Frame.Parent = ScreenGui
Frame.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
Frame.Position = UDim2.new(0.284313738, 0, 0.326579273, 0)
Frame.Size = UDim2.new(0, 495, 0, 285)
Frame.Active = true
Frame.Draggable = true

TextLabel.Parent = Frame
TextLabel.BackgroundColor3 = Color3.fromRGB(73, 112, 255)
TextLabel.Size = UDim2.new(0, 495, 0, 46)
TextLabel.Font = Enum.Font.SourceSans
TextLabel.Text = "Made By | Baconisthebestmatter | MM2"
TextLabel.TextColor3 = Color3.fromRGB(0, 0, 0)
TextLabel.TextSize = 14.000

UICorner.Parent = TextLabel

UICorner_2.Parent = Frame

eclipse.Name = "eclipse"
eclipse.Parent = Frame
eclipse.BackgroundColor3 = Color3.fromRGB(35, 255, 60)
eclipse.Position = UDim2.new(0.0484848469, 0, 0.378947377, 0)
eclipse.Size = UDim2.new(0, 200, 0, 106)
eclipse.Font = Enum.Font.PermanentMarker
eclipse.Text = "Eclipes MM2"
eclipse.TextColor3 = Color3.fromRGB(0, 0, 0)
eclipse.TextScaled = true
eclipse.TextSize = 14.000
eclipse.TextWrapped = true
eclipse.MouseButton1Down:connect(function()
loadstring(game:HttpGet("https://raw.githubusercontent.com/Ethanoj1/EclipseMM2/master/Script", true))()
end)

UICorner_3.Parent = eclipse

Vynixu.Name = "Vynixu"
Vynixu.Parent = Frame
Vynixu.BackgroundColor3 = Color3.fromRGB(16, 255, 76)
Vynixu.Position = UDim2.new(0.51313132, 0, 0.378947377, 0)
Vynixu.Size = UDim2.new(0, 200, 0, 106)
Vynixu.Font = Enum.Font.PermanentMarker
Vynixu.Text = "Vynixu MM2"
Vynixu.TextColor3 = Color3.fromRGB(0, 0, 0)
Vynixu.TextScaled = true
Vynixu.TextSize = 14.000
Vynixu.TextWrapped = true
Vynixu.MouseButton1Down:connect(function()
loadstring(game:GetObjects("rbxassetid://4001118261")[1].Source)()
end)

UICorner_4.Parent = Vynixu
